{
  "name": "@passerelle/insider-nuxt",
  "version": "0.1.0",
  "description": "passerelle insider module for nuxt2.14+ or nuxt3",
  "repository": {
    "type": "git",
    "url": "git@github.com:hacomono-lib/passerelle-vue.git",
    "directory": "packages/insider-nuxt"
  },
